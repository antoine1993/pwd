<header>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3 top-social-icons text-center">
                <ul class="social-icons">
                    <li class="mr-1"><a href={{ path('profile') }} ><span class="glyphicon glyphicon-user"></span>Profile</a></li>
                    <li class="mr-1"><a href={{ path('game_gallery') }} ><span class="fab fa-admin"></span>Admin</a></li>

                </ul>
            </div>
            <div class="col-md-6 logo text-center">
                <h1>
                    <a class="navbar-brand" href={{ path('home') }} >
                        <i class="fas fa-leaf"></i>Arable</a>

                </h1>

            </div>
            <div class="col-md-3 login-right-img text-center">
                <a class="request text-uppercase" href={{ path('fos_user_security_login') }} >LOGIN</a>
                <a class="request text-uppercase" href={{ path('fos_user_registration_register') }} >REGISTER</a>
            </div>
        </div>
    </div>
    <div class="header-bg">
        <div class="container">
            <header>
                <nav class="navbar navbar-expand-lg navbar-light">
                    <button class="navbar-toggler navbar-toggler-right mx-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
